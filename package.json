{
  "name": "react-typescript",
  "author": "KevinGwon",
  "email": "KevinGwon@gamil.com",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "server": "cross-env NODE_ENV=development nodemon --exec babel-node express/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinGwon/react-portfolio.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "globals": {
      "IG": false,
      "TweenMax": false,
      "TimelineMax": false
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "no-unused-vars": 1,
      "no-console": 0,
      "react/jsx-filename-extension": 0,
      "eslint-disable-next-line jsx-a11y/anchor-is-valid": 0,
      "no-useless-concat": 0,
      "jsx-a11y/anchor-is-valid": 0
    }
  },
  "dependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "express": "^4.17.1",
    "gsap": "^3.0.4",
    "nodemon": "^2.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^4.5.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "imagesloaded": "^4.1.4",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.13.0",
    "node-sass-glob-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-star-ratings": "^2.3.0",
    "sass-loader": "^8.0.0",
    "sass-mq": "^5.0.1",
    "style-loader": "^1.0.2",
    "swiper": "4.5.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "keywords": [],
  "bugs": {
    "url": "https://github.com/kevinGwon/react-typescript/issues"
  },
  "homepage": "https://github.com/kevinGwon/react-typescript#readme"
}
