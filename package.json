{
  "name": "react-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:kevinGwon/react-typescript.git",
  "author": "kevinGwon <developerGwon@gmail.com>",
  "license": "MIT",
  "scripts": {
    "server": "cross-env NODE_ENV=development node server.js",
    "dev": "cross-env NODE_ENV=development next",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "deploy": "npm run build && npm run export"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "globals": {
      "IG": false,
      "TweenMax": false,
      "TimelineMax": false
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "no-unused-vars": 1,
      "no-console": 0,
      "react/jsx-filename-extension": 0,
      "eslint-disable-next-line jsx-a11y/anchor-is-valid": 0,
      "no-useless-concat": 0,
      "jsx-a11y/anchor-is-valid": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "express": "^4.17.1",
    "gsap": "^3.1.1",
    "imagesloaded": "^4.1.4",
    "next": "^9.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-star-ratings": "^2.3.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "next-redux-wrapper": "^4.0.1",
    "next-routes": "^1.4.2",
    "node-sass": "^4.13.1",
    "node-sass-glob-importer": "^5.3.2",
    "nodemon": "^2.0.2",
    "normalize.css": "^8.0.1",
    "now": "^16.7.3",
    "prettier": "^1.19.1",
    "redux-devtools-extension": "^2.13.8",
    "sass-mq": "^5.0.1",
    "styled-components": "^5.0.1",
    "swiper": "^4.5.0",
    "typesafe-actions": "^4.4.2",
    "typescript": "^3.7.5"
  }
}
